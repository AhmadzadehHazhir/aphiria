{
  "name": "aphiria/router",
  "description": "The Aphiria router library",
  "keywords": [
    "PHP",
    "router",
    "routing",
    "matching"
  ],
  "authors": [
    {
      "name": "David Young",
      "email": "dave@aphiria.com",
      "role": "Architect"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Aphiria\\Routing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aphiria\\Routing\\Tests\\": "tests"
    }
  },
  "require": {
    "ext-ctype": "*",
    "ext-mbstring": "*",
    "opis/closure": "3.1.*",
    "php": "^7.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.14",
    "nikic/fast-route": "1.2",
    "phpunit/phpunit": "~8.0",
    "symfony/routing": "4.2.*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev"
}